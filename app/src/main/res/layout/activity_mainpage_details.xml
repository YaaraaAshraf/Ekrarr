<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBEBEB"
    android:orientation="vertical"
    tools:context=".Main_bar">

    <TextView
    android:id="@+id/text_details"
    android:layout_width="match_parent"
    android:layout_height="44dp"
    android:layout_marginTop="20dp"
    android:paddingRight="20dp"
    android:text="@string/text_title"
    android:textColor="@color/grey"
    android:textSize="18dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="60dp"
        android:layout_marginRight="100dp"
        android:background="@drawable/squareshape"
        app:cardBackgroundColor="#e0e0e0"
        app:cardCornerRadius="4dp"
        app:cardElevation="3dp">


        <ImageView
            android:id="@+id/img_gm"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="20dp"
            android:src="@drawable/gm" />
        <TextView
        android:id="@+id/txt_gm"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/menuLayout_gm"
        android:layout_alignRight="@+id/menuLayout_gm"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="80dp"
        android:text="سند قبض نقدى"
        android:textColor="@color/grey" />
    </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginLeft="230dp"
            android:layout_marginTop="-126dp"
            android:background="@drawable/squareshape"
            app:cardBackgroundColor="#e0e0e0"
            app:cardCornerRadius="4dp"
            app:cardElevation="3dp">
            <ImageView
                android:id="@+id/image_gr"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/gr" />
            <TextView
                android:id="@+id/txt_gr"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignEnd="@+id/menuLayout_gm"
                android:layout_alignRight="@+id/menuLayout_gm"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="80dp"
                android:text="سند قبض عيني"
                android:textColor="@color/grey" />
        </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="338dp"
        android:layout_height="109dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="40dp"
        android:background="@drawable/squareshape"
        app:cardBackgroundColor="#e0e0e0"
        app:cardCornerRadius="4dp"
        app:cardElevation="3dp">

        <ImageView
            android:id="@+id/img_pr"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/pr" />

        <TextView
            android:id="@+id/txt_pr"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignEnd="@+id/menuLayout_gm"
            android:layout_alignRight="@+id/menuLayout_gm"
            android:layout_marginLeft="86dp"
            android:layout_marginTop="80dp"
            android:text="المنتجات"
            android:textColor="@color/grey" />
    </androidx.cardview.widget.CardView>

</LinearLayout>


